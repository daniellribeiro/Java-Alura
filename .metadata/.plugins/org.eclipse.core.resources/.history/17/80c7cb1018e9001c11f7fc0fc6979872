import java.util.ArrayList;
import java.util.List;
import java.util.Optional;
import java.util.function.Supplier;

public class Teste3 {
	public static void main(String[] args) {
		List<Curso> cursos = new ArrayList<Curso>();
		cursos.add(new Curso("Python",45));
		cursos.add(new Curso("JavaScript",150));
		cursos.add(new Curso("Java 8",113));
		cursos.add(new Curso("C",55));
		
		//cursos.sort(Comparator.comparingInt(Curso::getAlunos));
		
		//cursos.forEach(c -> System.out.println(c.getNome()));
		
		//cursos.stream().filter(c -> c.getAlunos() > 50).forEach(c -> System.out.println(c.getNome()));
		
		String cursoFiltrado;
		
		try{
		  cursoFiltrado = cursos.stream()
				.filter(c -> c.getAlunos() > 500)
				.findFirst().get().getNome();
		}
		catch(NullPointerException ex){
			cursoFiltrado = "SEM RETORNO";
		}
		
			System.out.println(cursoFiltrado);
			
	}
}
