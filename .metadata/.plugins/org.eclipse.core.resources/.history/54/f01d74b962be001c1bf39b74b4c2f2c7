package curso1;


public class TestaCurso {

    public static void main(String[] args) {
        Curso curso = new Curso("Curso1","professor1");
        
        curso.getAulas().add(new Aula("Aula1",21));
        

        curso.getAulas().add(new Aula("Aula2",21));
        

        curso.getAulas().add(new Aula("Aula3",21));
        
        System.out.println(curso);
    }
}