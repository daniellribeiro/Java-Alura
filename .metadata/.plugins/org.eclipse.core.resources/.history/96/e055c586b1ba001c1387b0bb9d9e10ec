package curso1;

public class Aula implements Comparator<Aula>{
	private String titulo;
	private int tempo;
	
	public Aula(String titulo, int tempo){
		this.titulo = titulo;
		this.tempo = tempo;
	}
	
	@Override
	public String toString(){
		return "\n Aula: " + this.titulo + ", tempo: " + this.tempo;
	}
	
	
}
