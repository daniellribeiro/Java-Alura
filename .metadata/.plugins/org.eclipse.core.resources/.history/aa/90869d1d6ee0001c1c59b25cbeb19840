import java.util.ArrayList;
import java.util.List;
import java.util.function.Consumer;

public class aula2 {
	public static void main(String[] args) {
		List palavras = new ArrayList();
		palavras.add("a");
		palavras.add("b");
		palavras.add("c");
	
		palavras.forEach(s ->
	    	System.out.println(s)
		);
	}
}
